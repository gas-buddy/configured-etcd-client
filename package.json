  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-gasbuddy": "^3.3.3",
    "eslint-config-gasbuddy": "^4.1.0",
    "tap": "^11.1.5",
    "winston": "^2.4.3"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "microlock": "^1.1.0",
    "node-etcd": "^5.0.3",
    "uuid": "^3.3.2"
  }
